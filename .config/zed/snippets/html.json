{
  "HTML5 Boilerplate": {
    "prefix": "html5",
    "scope": "html",
    "body": [
      "<!DOCTYPE html>",
      "<html lang=\"en\">",
      "<head>",
      "    <meta charset=\"UTF-8\">",
      "    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">",
      "    <title>${1:Document}</title>",
      "</head>",
      "<body>",
      "    $0",
      "</body>",
      "</html>"
    ]
  },
    "div": {
      "prefix": "div",
      "body": "<div class=\"$1\">\n\t$2\n</div>\n\t$0"
    },
    "container": {
      "prefix": "container",
      "body": "<div class=\"container\">\n\t$1\n</div>\n\t$0",
      "description": "container"
    },
    "container-fluid": {
      "prefix": "container",
      "body": "<div class=\"container-fluid\">\n\t$1\n</div>\n\t$0",
      "description": "container-fluid"
    },
    "row": {
      "prefix": "row",
      "body": "<div class=\"row\">\n\t$1\n</div>\n\t$0"
    },
    "col": {
      "prefix": "col",
      "body": "<div class=\"col$1\">\n\t$2\n</div>\n\t$0"
    },
    "span": {
      "prefix": "span",
      "body": "<span class=\"$1\">\n\t$2\n</span>\n\t$0"
    },
    "h1": {
      "prefix": "h1",
      "body": "<h1>$1</h1>\n\t$0"
    },
    "h2": {
      "prefix": "h2",
      "body": "<h2>$1</h2>\n\t$0"
    },
    "h3": {
      "prefix": "h3",
      "body": "<h3>$1</h3>\n\t$0"
    },
    "h4": {
      "prefix": "h4",
      "body": "<h4>$1</h4>\n\t$0"
    },
    "h5": {
      "prefix": "h5",
      "body": "<h5>$1</h5>\n\t$0"
    },
    "small": {
      "prefix": "small",
      "body": "<small>$1</small>$0"
    },
    "p": {
      "prefix": "<p",
      "body": "<p class=\"\">\n\t$1\n</p>\n\t$0"
    },
    "br": {
      "prefix": "br",
      "body": "<br/>\n$0"
    }
}
